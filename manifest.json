{
    "manifest_version": 3,
    "name": "AI parental control",
    "version": "1.0.0",
    "action": 
    { 
        "default_popup": "index.html",
        "default_icon": "icon_white.png"
    },
    "content_scripts": [
        {
          "js": ["content.js"],
          "matches": [
            "http://*/*",
            "https://*/*"
          ],
          "run_at": "document_start"
        }
    ],

    "permissions": [
        "storage"
    ]
}